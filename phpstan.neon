includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:

    paths:
        - app

    # The level 9 is the highest level
    level: 5

    ignoreErrors:
        - '#Call to an undefined method Spatie[a-zA-Z0-9\\_]+::forUrls\(\)#'

    excludePaths:
        #- ./*/*/app/Http/Resources/*
        - *Resource.php

    checkMissingIterableValueType: false
